<div class="pagination">
  <ul>
    <li class="{{#unless prev}}disabled{{/unless}}">
        {{#if prev}}
            <a href="{{prev}}" title="Previous Page">&laquo;</a>
        {{else}}<a>&laquo;</a>{{/if}}
    </li>
    {{#pages}}
    <li class="{{#active}}active{{/active}}">
        <a href="{{url}}">{{page}}</a>
    </li>
    {{/pages}}
    <li class="{{#unless next}}disabled{{/unless}}">
        {{#if next}}
            <a href="{{next}}" title="Next Page">&raquo;</a>
        {{else}}<a>&raquo;</a>{{/if}}
    </li>
  </ul>
</div>
